<p id="notice"><%= notice %></p>

<h1>Gazelle Runners</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gazelle_runners.each do |gazelle_runner| %>
      <tr>
        <td><%= link_to 'Show', gazelle_runner %></td>
        <td><%= link_to 'Edit', edit_gazelle_runner_path(gazelle_runner) %></td>
        <td><%= link_to 'Destroy', gazelle_runner, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

